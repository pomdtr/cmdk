{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cmdk",
  "title": "cmdk",
  "icon": "command-icon.png",
  "author": "pomdtr",
  "license": "MIT",
  "description": "CMDK Raycast extension",
  "commands": [
    {
      "name": "open-page",
      "title": "Search Cmdk Pages",
      "description": "List all actions",
      "mode": "view",
      "arguments": [
        {
          "type": "text",
          "name": "url",
          "placeholder": "URL",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.71.4",
    "@raycast/utils": "^1.14.0",
    "cross-fetch": "^4.0.0",
    "fzf": "^0.5.2",
    "lowdb": "^7.0.1",
    "nanoid": "^5.0.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "tsx": "^4.9.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
